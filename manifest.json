{
  "manifest_version": 3,
  "name": "TanStack Query DevTools",
  "version": "0.1.0",
  "description": "Chrome DevTools extension for debugging TanStack Query applications",
  "permissions": [
    "activeTab"
  ],
  "devtools_page": "src/devtools/devtools.html",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content/content.js"],
      "run_at": "document_start"
    }
  ],
  "background": {
    "service_worker": "background/background.js"
  },
  "web_accessible_resources": [
    {
      "resources": ["injected/injected.js"],
      "matches": ["<all_urls>"]
    }
  ],
  "icons": {
    "16": "public/vite.svg",
    "48": "public/vite.svg",
    "128": "public/vite.svg"
  }
}
